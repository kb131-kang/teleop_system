<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Fingertip Macro: default type (simple tip) -->
  <xacro:macro name="dg5f_left_tip_default" params="prefix finger_num parent_link origin_xyz origin_rpy">
    <link name="${prefix}ll_dg_${finger_num}_tip">
      <inertial>
        <origin xyz="0.0038063 5.479E-07 0.007157" rpy="0.0 0.0 0.0" />
        <inertia ixx="3.2445E-07" ixy="2.1191E-12" ixz="1.3375E-08" iyy="1.9895E-07" iyz="-3.0731E-11" izz="2.5698E-07" />
        <mass value="0.005" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="${origin_rpy}" />
        <geometry>
          <mesh filename="package://dg_description/meshes/dg5f_left_base/visual/dg_tip.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="${origin_rpy}" />
        <geometry>
          <mesh filename="package://dg_description/meshes/dg5f_left_base/collision/dg_tip_c.STL" />
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}lj_dg_${finger_num}_tip" type="fixed">
      <origin xyz="${origin_xyz}" rpy="0.0 0.0 0.0" />
      <parent link="${parent_link}" />
      <child link="${prefix}ll_dg_${finger_num}_tip" />
    </joint>
  </xacro:macro>

