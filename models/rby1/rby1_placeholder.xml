<!-- RB-Y1 Humanoid Robot with DG-5F Dexterous Hands - MuJoCo Model
     Rainbow Robotics RB-Y1: Mobile base + 6-DOF torso + 2x7-DOF arms + 2x20-DOF hands + 2-DOF head
     Total: 62 actuated joints (no free joint - base fixed for sim testing)

     Joint index mapping (qpos/ctrl):
       [0:6]   Torso (6 joints)
       [6:13]  Left arm (7 joints)
       [13:33] Left hand DG-5F (20 joints: 5 fingers x 4)
       [33:40] Right arm (7 joints)
       [40:60] Right hand DG-5F (20 joints: 5 fingers x 4)
       [60:62] Head pan-tilt (2 joints)

     NOTE: Mobile base is fixed in this model. For locomotion testing,
     add a freejoint to base_link and adjust index offsets accordingly.
-->
<mujoco model="rby1_dg5f_teleop">
  <compiler angle="radian" autolimits="true" />

  <option timestep="0.002" gravity="0 0 -9.81">
    <flag contact="enable" />
  </option>

  <default>
    <joint damping="5" armature="0.01" />
    <geom condim="4" friction="1 0.5 0.01" />

    <default class="torso">
      <joint damping="20" armature="0.1" />
      <geom rgba="0.25 0.25 0.30 1" />
    </default>

    <default class="arm">
      <joint damping="10" armature="0.05" />
      <geom rgba="0.45 0.45 0.50 1" />
    </default>

    <default class="hand">
      <joint damping="1" armature="0.002" />
      <geom rgba="0.65 0.65 0.70 1" />
    </default>

    <default class="head">
      <joint damping="5" armature="0.02" />
      <geom rgba="0.20 0.20 0.22 1" />
    </default>
  </default>

  <asset>
    <texture name="grid_tex" type="2d" builtin="checker" width="512" height="512"
             rgb1="0.85 0.85 0.85" rgb2="0.70 0.70 0.70" />
    <material name="grid_mat" texture="grid_tex" texrepeat="8 8" reflectance="0.1" />
    <material name="base_mat" rgba="0.15 0.15 0.18 1" />
    <material name="joint_mat" rgba="0.10 0.10 0.60 1" />
    <material name="camera_mat" rgba="0.08 0.08 0.08 1" />
    <material name="finger_mat" rgba="0.70 0.70 0.75 1" />
    <material name="palm_mat" rgba="0.55 0.55 0.60 1" />
